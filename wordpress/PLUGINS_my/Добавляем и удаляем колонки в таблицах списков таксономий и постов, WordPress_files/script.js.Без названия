function finishedLesson() {

	if( ! jQuery( 'body' ).hasClass( 'single-course' ) ) {
		return false;
	}

	var nonce = jQuery( '#nonce').val(),
			lesson_id = jQuery( '#lesson-id').val();

	if( jQuery( '#lesson-li-' + lesson_id ).hasClass( 'lesson-finished' ) ) {
		return false;
	}

	jQuery.ajax({
		type: 'POST',
		url: misha_object.ajaxurl,
		data: {
			'action' : 'finishedlesson',
			'nonce' : nonce,
			'lesson_id' : lesson_id
		},
		success: function(data){
			console.log( 'Lesson finished.' );
			if( data && 'yes' == data ) {
				jQuery( '#lesson-li-' + lesson_id ).addClass( 'lesson-finished' );
			}
		}
	});

}

jQuery(function($){

	setTimeout( finishedLesson, 300000 );

	if( $('.lines').length ) {
		$('.lines').each(function(i){
			//console.log( Math.floor( parseInt($(this).height()) / 24 ) );
			var synt = $(this),
				lines = '',
				syntLines = synt.find('span'),
				syntLinesStart = parseInt(syntLines.attr('data-start'));
				syntLinesMax = parseInt(syntLines.attr('data-max'));
			if( syntLines.length && syntLinesStart && syntLinesMax ) {

				for (i = 0; i < syntLinesMax; i++) {
					lines += syntLinesStart+i+"\n";
				}
				synt.html('<pre>' + lines + '</pre>');
			}
		});
	}





var clickDelay      = 500,
	clickDelayTimer = null;
	$('.falafel').on('click', function () {

  if(clickDelayTimer === null) {

    var $burger = $(this);
    $burger.toggleClass('active');
    $burger.parent().toggleClass('is-open');

    if(!$burger.hasClass('active')) {
      $burger.addClass('closing');
			$('.super_header_nav').slideUp(100);
    }else{
			$('.super_header_nav').slideDown(100);
		}

    clickDelayTimer = setTimeout(function () {
      $burger.removeClass('closing');

      clearTimeout(clickDelayTimer);
      clickDelayTimer = null;
    }, clickDelay);
  }
});


